{
  "name": "schemock",
  "version": "0.1.0",
  "description": "Schema-first code generation for frontend developers. Generate TypeScript types, API clients, React hooks, Next.js routes, and Node.js handlers from a single schema.",
  "keywords": [
    "schema",
    "codegen",
    "mock",
    "typescript",
    "api",
    "react",
    "nextjs",
    "express",
    "supabase",
    "frontend",
    "backend",
    "code-generation",
    "crud",
    "rest-api"
  ],
  "author": "Prajyot Tote",
  "license": "MIT",
  "homepage": "https://github.com/prajyot-tote/schemock#readme",
  "bugs": {
    "url": "https://github.com/prajyot-tote/schemock/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prajyot-tote/schemock.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./schema": {
      "types": "./dist/schema/index.d.ts",
      "import": "./dist/schema/index.mjs",
      "require": "./dist/schema/index.js"
    },
    "./runtime": {
      "types": "./dist/runtime/index.d.ts",
      "import": "./dist/runtime/index.mjs",
      "require": "./dist/runtime/index.js"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": "./dist/adapters/index.mjs",
      "require": "./dist/adapters/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware/index.d.ts",
      "import": "./dist/middleware/index.mjs",
      "require": "./dist/middleware/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.mjs",
      "require": "./dist/cli/index.js"
    },
    "./seed": {
      "types": "./dist/seed/index.d.ts",
      "import": "./dist/seed/index.mjs",
      "require": "./dist/seed/index.js"
    }
  },
  "bin": {
    "schemock": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest --exclude '**/e2e/**'",
    "test:run": "vitest run --exclude '**/e2e/**'",
    "test:integration": "vitest run src/__tests__/integration/",
    "test:integration:watch": "vitest src/__tests__/integration/",
    "test:e2e": "vitest run src/__tests__/e2e/",
    "test:e2e:watch": "vitest src/__tests__/e2e/",
    "test:all": "vitest run",
    "pretest:integration": "npm run build",
    "pretest:e2e": "npm run build",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "verify-dist": "node scripts/verify-dist.mjs",
    "prepublishOnly": "npm run build && npm run verify-dist"
  },
  "peerDependencies": {
    "@electric-sql/pglite": "^0.3.0",
    "@faker-js/faker": "^8.0.0",
    "@tanstack/react-query": "^5.0.0",
    "msw": "^2.0.0",
    "react": "^18.0.0",
    "zod": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@electric-sql/pglite": {
      "optional": true
    },
    "@faker-js/faker": {
      "optional": true
    },
    "@tanstack/react-query": {
      "optional": true
    },
    "msw": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@electric-sql/pglite": "^0.3.15",
    "@faker-js/faker": "^10.1.0",
    "@mswjs/data": "^0.16.2",
    "@supabase/supabase-js": "^2.90.1",
    "@tanstack/react-query": "^5.90.12",
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.7",
    "msw": "^2.12.4",
    "react": "^19.2.3",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
